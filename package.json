    "front-end",
    "frontend",
    "material-ui",
    "material",
    "mui",
    "react.js",
    "react",
    "ui"
  ],
  "homepage": "https://github.com/aiden-bok/almui",
  "bugs": {
    "url": "https://github.com/aiden-bok/almui/issues",
    "email": "bokjaeoh@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aiden-bok/almui.git"
  },
  "author": "Aiden <bokjaeoh@gmail.com>",
  "license": "ISC",
  "engines": {
    "node": ">= 18.16.1"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-react": "^7.22.15",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "moment": "^2.29.4",
    "prettier": "^3.0.3",
    "prettier-eslint": "^15.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.68.0"
  },
  "peerDependencies": {
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.68.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "format": "prettier --check ./",
    "format:fix": "prettier --write ./",
    "lint": "eslint src/**",
    "dist": "rm -rf dist && mkdir dist && babel src -d dist --copy-files",
    "start": "npm run dist && cd demos && npm start"
